{
  "name": "rustable",
  "version": "0.0.1",
  "description": "Rust-inspired features implemented in TypeScript, including traits, Option, Result, and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "node scripts/build.js",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepublishOnly": "npm run test && npm run build",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "rust",
    "traits",
    "option",
    "result",
    "type-safe",
    "functional"
  ],
  "author": "illuxiza",
  "repository": {
    "type": "git",
    "url": "https://github.com/illuxiza/rustable.git"
  },
  "bugs": {
    "url": "https://github.com/illuxiza/rustable/issues"
  },
  "homepage": "https://github.com/illuxiza/rustable#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.5.0"
}
