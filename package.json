{
  "name": "rustable",
  "version": "0.0.2",
  "description": "A TypeScript library that brings Rust-inspired features to TypeScript, including traits, Option, Result, and more. Zero dependencies, type-safe implementation.",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && node scripts/build.mjs",
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "format": "prettier --write src/**/*.ts test/**/*.ts",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "npm run build"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "rust",
    "traits",
    "option",
    "result",
    "type-safe",
    "functional",
    "pattern-matching",
    "error-handling",
    "zero-dependencies"
  ],
  "author": "illuxiza",
  "repository": {
    "type": "git",
    "url": "https://github.com/illuxiza/rustable.git"
  },
  "bugs": {
    "url": "https://github.com/illuxiza/rustable/issues"
  },
  "homepage": "https://github.com/illuxiza/rustable#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.5.0"
}
